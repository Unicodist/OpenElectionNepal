akka.actor.deployment {
    /vote {
        router = round-robin-group
        routees.paths = ["/user/vote"]
        cluster {
            enabled = on
            use-role = Central
            allow-local-routees = off
        }
    }
    /booth-coordinator {
        router = round-robin-group
        routees.paths = ["/user/booth-coordinator"]
        cluster {
            enabled = on
            use-role = Central
            allow-local-routees = off
        }
    }
}